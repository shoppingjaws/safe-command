{
  "name": "safe-command",
  "version": "0.1.3",
  "description": "A proxy tool to safely restrict commands for AI agents",
  "author": "safe-command contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shoppingjaws/safe-command.git"
  },
  "keywords": [
    "security",
    "command",
    "proxy",
    "ai",
    "claude-code",
    "allowlist",
    "aws",
    "cli"
  ],
  "module": "src/index.ts",
  "type": "module",
  "bin": {
    "safe-command": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --compile --outfile safe-command",
    "test": "bun test",
    "lint": "bunx biome check src/",
    "lint:fix": "bunx biome check --write src/"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@types/bun": "latest",
    "@types/js-yaml": "^4.0.9"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
